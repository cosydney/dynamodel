<p id="notice"><%= notice %></p>


<h1>Test </h1>
<% @mannequin.photos.each do |photo| %>
<%= cl_image_tag photo.path, width: 300, height: 200, crop: :fill %>
<% end %>

<h1>Listing Photos</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @attachinaryfiles.each do |attachinary_file| %>
      <tr>
      <td><%= attachinary_file.photos  %> </td>
      <%= cl_image_tag attachinary_file.path, width: 300, height: 200, crop: :fill %>

        <td><p><strong><%= attachinary_file.description %></strong></p></td>
        <td><%= link_to 'Show', attachinary_file %> ||</td>
        <td><%= link_to 'Edit', edit_attachinary_file_path(attachinary_file) %> ||</td>
        <td><%= link_to 'Destroy', attachinary_file, method: :delete%> ||</td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New pictures', new_mannequin_attachinary_file_path %>
