<!-- making a partial to display each categories -->

<div class="index-wrapper">
  <button class="btn-white-category"> Haute-Couture </button>
  <br>
  <div class="container">
    <div class="row">
      <% category.each do |m| %>
        <div class="col-sm-6 col-md-3">
          <div class="mannequin-card">
          <!-- link_to not working properly -->
          <p><%= link_to mannequin_path(m) do %>
            <%= cl_image_tag m.photos[0].path, width: 200, height: 200, crop: :fill %>
            <div> <%= "#{m.first_name + " " + m.last_name[0] + "."}" %> </div>
            <% end %>
          </div>
        </p>
      </div>
      <% end %>
    </div>
  </div>
</div>
<hr>


